<%= stylesheet_link_tag 'LoginPage', media: 'all', 'data-turbolinks-track': 'reload' %>

<main class="idgroup11 divstyl15">
  <div class="idgroup10 idgroup23 divstyl14 divstyl27">
    <div class="idgroup9 divstyl10">
      <a>
        <div>
          <%= image_tag"photo1.png" %>
        </div>
      </a>
    </div>
    <div class="idgroup9 divstyl11 idgroup24 divstyl28">
      <div class="idgroup18 divstyl22">
        <div class="idgroup10 divstyl14">
          <div class="idgroup9 divstyl12">
            <div class="idgroup19 divstyl23">
              <h4 class="idgroup h4conn" font-weight="bold">Connectez-vous à Avito</h4>
              <p class="idgroup pstyl">Vous pouvez utiliser l'authentification rapide avec votre compte Facebook ou Gmail.</p>
            </div>
          </div>
          <div class="idgroup9 divstyl13">
            <div class="idgroup13 divstyl17">
              <button class="idgroup12 divstyl16">
                <svg height="24" width="24" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 48 48" class="LgbsSe-Bz112c"><g><path fill="#EA4335" d="M24 9.5c3.54 0 6.71 1.22 9.21 3.6l6.85-6.85C35.9 2.38 30.47 0 24 0 14.62 0 6.51 5.38 2.56 13.22l7.98 6.19C12.43 13.72 17.74 9.5 24 9.5z"></path><path fill="#4285F4" d="M46.98 24.55c0-1.57-.15-3.09-.38-4.55H24v9.02h12.94c-.58 2.96-2.26 5.48-4.78 7.18l7.73 6c4.51-4.18 7.09-10.36 7.09-17.65z"></path><path fill="#FBBC05" d="M10.53 28.59c-.48-1.45-.76-2.99-.76-4.59s.27-3.14.76-4.59l-7.98-6.19C.92 16.46 0 20.12 0 24c0 3.88.92 7.54 2.56 10.78l7.97-6.19z"></path><path fill="#34A853" d="M24 48c6.48 0 11.93-2.13 15.89-5.81l-7.73-6c-2.15 1.45-4.92 2.3-8.16 2.3-6.26 0-11.57-4.22-13.47-9.91l-7.98 6.19C6.51 42.62 14.62 48 24 48z"></path><path fill="none" d="M0 0h48v48H0z"></path></g></svg>
                <span>Se connecter</span>
              </button>
              <div class="idgroup2 divstyl">
                <button class="idgroup12 divstyl16">
                  <svg height="24" width="24" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg" aria-labelledby="FacebookTitleID" style="fill: currentcolor; stroke: currentcolor; stroke-width: 0;">
                    <title>Facebook Icon</title>
                    <path d="M14 13.5h2.5l1-4H14v-2c0-1.03 0-2 2-2h1.5V2.14c-.326-.043-1.557-.14-2.857-.14C11.928 2 10 3.657 10 6.7v2.8H7v4h3V22h4v-8.5z"></path>
                  </svg>
                  <span>Se connecter</span>
                </button>
              </div>
            </div>
          </div>
          <div class="idgroup9 divstyl13">
            <div class="idgroup15 divstyl19">
              <div class="idgroup16 divstyl20">
              </div>
              <div class="idgroup17">OU BIEN
              </div>
              <div class="idgroup16 divstyl20">
              </div>
            </div>
          </div>
          <div class="idgroup9 divstyl12">
            <%= form_with url: login_path do |form| %>
              <p class="idgroup">Entrez votre e-mail et mot de passe</p>
              <div class="idgroup2 divstyl">
                <label  class="idgroup3 divstyl2">E-mail</label>
                <div tabindex="-1" class="idgroup6 divstyl6">
                  <div class="idgroup7 divstyl7">
                    <svg height="24" width="24" viewBox="0 0 24 24" style="fill: rgb(155, 155, 155); stroke: rgb(155, 155, 155); stroke-width: 0;">
                      <title id="EmailTitleID">Email Icon</title>
                      <path d="M18.4 6c.88 0 1.6.72 1.6 1.6v9.6c0 .88-.72 1.6-1.6 1.6H5.6c-.88 0-1.6-.72-1.6-1.6l.008-9.6c0-.88.712-1.6 1.592-1.6h12.8zm-.8 11.2c.44 0 .8-.36.8-.8V9.2l-5.552 3.472a1.585 1.585 0 01-1.696 0L5.6 9.2v7.2c0 .44.36.8.8.8h11.2zM12 11.6l6.4-4H5.6l6.4 4z"></path>
                    </svg>
                  </div>
                  <input type="email" name="E-mail" placeholder="E-mail" value="" class="idgroup4 divstyl3">
                  <div class="idgroup7 divstyl8"></div>
                </div>
              </div>
              <div class="idgroup2 divstyl">
                <label class="idgroup3 divstyl2">Mot de passe</label>
                <div tabindex="-1" class="idgroup6 divstyl6">
                  <div class="idgroup7 divstyl7">
                    <svg height="24" width="24" viewBox="0 0 24 24" style="fill: rgb(155, 155, 155); stroke: rgb(155, 155, 155); stroke-width: 0;">
                      <title>Unlock Icon</title>
                      <path d="M12 17a2 2 0 002-2 2 2 0 00-2-2 2 2 0 00-2 2 2 2 0 002 2m6-9a2 2 0 012 2v10a2 2 0 01-2 2H6a2 2 0 01-2-2V10a2 2 0 012-2h1V6a5 5 0 015-5 5 5 0 015 5v2h1m-6-5a3 3 0 00-3 3v2h6V6a3 3 0 00-3-3z"></path>
                    </svg>
                  </div>
                  <input type="password" name="Mot de passe" placeholder="Mot de passe" required="" value="" class="idgroup4 divstyl4">
                  <div class="idgroup7 divstyl8">
                    <div class="idgroup8 divstyl9">
                      <svg class="idgroup5 divstyl5" height="32" width="32" viewBox="0 0 24 24" style="fill: rgb(45, 45, 45); stroke: rgb(45, 45, 45); stroke-width: 0;">
                        <title>VisibilityOff Icon</title>
                        <path d="M2.71 3.16c.39-.39 1.03-.39 1.42 0l16.31 16.33a.996.996 0 11-1.41 1.41l-2.72-2.72c-1.34.52-2.79.82-4.31.82-5 0-9.27-3.11-11-7.5.77-1.97 2.06-3.67 3.68-4.96L2.71 4.57a.996.996 0 010-1.41zM12 16.5c.77 0 1.49-.18 2.14-.5l-1.57-1.57c-.19.04-.37.07-.57.07-1.66 0-3-1.34-3-3 0-.2.03-.39.06-.57L7.49 9.36C7.18 10 7 10.73 7 11.5c0 2.76 2.24 5 5 5zm0-10c-.51 0-1 .1-1.47.24L8.36 4.57C9.51 4.2 10.73 4 12 4c5 0 9.27 3.11 11 7.49-.69 1.76-1.79 3.3-3.18 4.53l-3.06-3.06c.14-.46.24-.95.24-1.46 0-2.76-2.24-5-5-5zm2.97 4.67a2.97 2.97 0 00-2.64-2.64l2.64 2.64z">
                        </path>
                      </svg>
                    </div>
                  </div>
                </div>
              </div>
              <span class="idgroup20 divstyl24">Mot de passe oublié ?</span>
              </form>
            <% end %>
          </div>
          <div class="idgroup9 divstyl12">
            <button type="submit" class="seconnid seconnid2 seconn seconnclass">Se connecter</button>
          </div>
          <div class="idgroup9 divstyl12">
            <div class="idgroup21 divstyl25">Pas encore inscrit?
              <%= link_to "S'inscrire gratuitement.", signup_path, class: "idgroup22 divstyl26" %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div>
  </div>
</main>